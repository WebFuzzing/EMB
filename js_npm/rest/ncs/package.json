{
  "name": "ncs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "evomaster-client-js": "1.5.0",
    "jest": "24.9.0",
    "dedent": "0.7.0",
    "superagent": "5.2.2",
    "supertest": "4.0.2",
    "rimraf": "^3.0.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "em:build": "rimraf instrumented && babel  src -d instrumented/src --copy-files && babel em -d instrumented/em --copy-files",
    "em:run": "node instrumented/em/em-main.js"
  },
  "babel": {
    "only": [
      "src"
    ],
    "plugins": [
      "module:evomaster-client-js"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "em/.*Test\\.(js|jsx|ts|tsx)$",
    "collectCoverageFrom": [
      "src/**/*.(js|jsx|ts|tsx)"
    ],
    "coverageReporters": [
      "json",
      "html",
      "json-summary"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/tests/",
      "<rootDir>/check/",
      "<rootDir>/build/",
      "<rootDir>/em"
    ]
  }
}
